<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping> 
      <!-- com.geeksforgeeks.GeekUserDetails  
         is the name of the java class 
          GeekUserDetails is the name of the table -->
    <class name="com.src.products.Product" table="product"> 
        <id name="Idproduct" type="int" column="idproduct"> 
            <generator class="assigned"/> 
        </id> 
        <property name="p_name" type="string"> 
            <column name="p_name"/> 
        </property> 
        <property name="p_packket"  type="string"> 
            <column name="p_packket"/> 
        </property> 
        <property name="p_address" type="string"> 
            <column name="p_address"/> 
        </property> 
        <property name="date" type="date"> 
            <column name="date"/> 
        </property> 
    </class> 
</hibernate-mapping>